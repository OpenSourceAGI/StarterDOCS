{
  "name": "nodejs-starter",
  "version": "0.0.0",
  "scripts": {
    "start": "clear && sudo forever --minUptime=10 --spinSleepTime=2000 -v -w app.js ",
    "mongo": "sudo service mongodb stop && mkdir db && sudo mongod --dbpath=db --smallfiles --nojournal",
    "admin": "sudo node node_modules/mongo-express/app.js"
  },
  "dependencies": {
    "body-parser": "~1.12.4",
    "cookie-parser": "~1.3.5",
    "debug": "^2.2.0",
    "ejs": "^2.3.1",
    "express": "~4.12.4",
    "express-session": "^1.11.3",
    "mongo-express": "^0.21.0",
    "mongoose": "^4.0.5",
    "passport": "^0.2.2",
    "passport-google-oauth2": "^0.1.6"
  }
}
