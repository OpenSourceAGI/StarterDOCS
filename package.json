{
  "name": "nodejs-starter",
  "version": "0.0.0",
  "scripts": {
    "start": "node app.js",
    "forever": "clear && sudo forever --minUptime=1000 --spinSleepTime=1000 -w app.js ",
    "mongo": "sudo service mongodb stop && mkdir db && mongod --dbpath=db --smallfiles --nojournal",
    "admin": "sudo node .\\node_modules\\mongo-express\\app.js"
  },
  "dependencies": {
    "body-parser": "~1.12.4",
    "cookie-parser": "~1.3.5",
    "debug": "^2.2.0",
    "ejs": "^2.3.1",
    "express": "~4.12.4",
    "express-session": "^1.11.3",
    "forever": "^0.14.1",
    "mongo-express": "^0.21.0",
    "mongoose": "^4.0.5",
    "passport": "^0.2.2",
    "passport-google-oauth": "^0.2.0",
    "passport-google-oauth2": "^0.1.6"
  }
}
