{
  "name": "fumadocs-starter",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build": "next build --turbo; bun run start",
    "build:api": "node ./scripts/generate-api-docs.mjs",
    "dev": "next dev --turbo",
    "start": "npx serve ../docs",
    "check": "fumadocs-mdx && tsc --noEmit"
  },
  "dependencies": {
    "@fumadocs/mdx-remote": "^1.4.0",
    "@orama/orama": "^3.1.11",
