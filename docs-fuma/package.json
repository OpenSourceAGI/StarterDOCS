    "fumadocs-mdx": "11.7.0",
    "fumadocs-openapi": "^9.1.4",
    "fumadocs-ui": "15.6.5",
    "lucide-react": "^0.525.0",
    "next": "15.4.0-canary.88",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "shiki": "^3.8.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/mdx": "^2.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3"
  }
