    "baseUrl": ".",
    "paths": {},
    "strict": false,
    "allowImportingTsExtensions": true,
    "target": "ESNext",
    "module": "ESNext",
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": false,
    "isolatedModules": false,
    "skipLibCheck": true,
